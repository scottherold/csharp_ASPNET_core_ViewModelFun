<!-- Arrays can be set as models like this -->

@model int[]

<div class="row m-4">
    <div class="col-lg-8 border border-dark rounded">
        <div class="row justify-content-center">
            <div class="col-lg-8 text-center m-4">
                <h2>Here Are Some Numbers</h2>
            </div>
        </div>
        <div class="row m-4">
            <div class="col-lg-4">
                <!-- Standard array logic can be passed to the Razor engine -->
                @for (int i = 0; i < Model.Length; i++)
                    {
                        <h3>@Model[i]</h3>
                    }
            </div>
        </div>
    </div>
</div>